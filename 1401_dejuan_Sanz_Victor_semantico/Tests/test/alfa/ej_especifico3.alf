// Tratamiento de vectores. Implementar un programa que lea 4 numeros y los almacene en un vector de 4 componentes. Posteriormente, el programa leera un dato y calculara el numero de apariciones de dicho dato en el vector.

main
{
// Declaraciones

    array int[4] vector1;
    int apariciones, i, x;


// Sentencias
	apariciones = 0;

	i = 0;
	while ((i<4))
	{
		scanf x;
		vector1[i] = x;
		i = i + 1;
	}

	scanf x;

	i = 0;
	while ((i<4))
	{
		if ((vector1[i] == x))
		{
			apariciones = apariciones + 1;
		}
		i = i + 1;
	}

	printf apariciones;
}
