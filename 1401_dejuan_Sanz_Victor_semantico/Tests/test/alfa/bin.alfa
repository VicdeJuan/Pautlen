// Introduce una secuencia ORDENADA de numeros y terminala con un 0 o metiendo
// 64 numeros. Despues mete un numero a buscar, responde posicion e iteraciones de busqueda binaria
main {

	array int [64] base;
	int contador;
	int s, buscar, total, iteraciones;
	boolean cond, res;

	function boolean esparada(int a){
		if((a==0)){
			return true;
		}else{
			return false;
		}
	}

	contador = 0;

	while ((contador<64)){
		base[contador] = 64;
		contador = contador + 1;
	}
	
	contador = 0;

	while ((contador<64)){
		scanf s;
		res = esparada(s);
		if(res){
			contador = 64;
		}else{
			base[contador] = s;
			contador = contador + 1;
		}
	}

	contador = 32;
	total = 32;
	cond = true;
	iteraciones = 0;

	printf 11111;
	scanf buscar;
	printf 11111;

	while (cond){
		if((base[contador]==buscar)){
			cond = false;
		}else{
			total = total / 2;
			iteraciones = iteraciones + 1;
			if((base[contador]<buscar)){
				contador = contador + total;
			}else{
				contador = contador - total;
			}
		}

		if((iteraciones >= 63) || (total == 0)){
			cond = false;
			contador = -1;
		}
	}

	printf contador;
	printf iteraciones;
}
